config fancontrol settings
    option enabled '1'
    option thermal_file '/sys/class/thermal/thermal_zone0/temp'
    option fan_file '/sys/class/hwmon/hwmon2/pwm1'
    option start_temp '55'
    option start_speed '80'
    option max_speed '255'
    option temp_div '1000'
    option monitor_interval '5'
    option temp_threshold '2'
    option max_temp '90'
    option curve_type '8'
